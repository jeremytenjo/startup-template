  "name": "startup-template",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.18.2",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev": "tsx ./devtools/scripts/dev/run.ts --dataSource=dev",
    "dev:signedIn": "tsx ./devtools/scripts/dev/run.ts --signedIn --dataSource=dev",
    "dev:dataSource-prod": "tsx ./devtools/scripts/dev/run.ts --dataSource=prod",
    "dev:onlyApp": "tsx ./devtools/scripts/dev/run.ts --onlyApp",
    "dev:for-testing": "tsx ./devtools/scripts/dev/run.ts --dataSource=prod --onlyApp",
    "storybook:dev": "tsx ./lib/integrations/Storybook/scripts/startStorybook/run.ts",
    "storybook:test:run": "tsx ./lib/integrations/Storybook/scripts/runStorybookTests/run.ts",
    "storybook:test:dev": "tsx ./lib/integrations/Storybook/scripts/runStorybookTestsWatch/run.ts",
    "storybook:doctor": "npx storybook doctor --config-dir lib/integrations/Storybook",
    "storybook:upgrade": "npx storybook@latest upgrade --config-dir lib/integrations/Storybook && npm run storybook:doctor",
    "build": "tsx ./devtools/scripts/buildApp/run.ts",
    "build:analyze": "ANALYZE=true next build",
    "build-and-serve": "npm run build && npm run serve",
    "serve": "next start",
    "tests:vitest:dev": "vitest --config ./lib/integrations/Vitest/vitest.config.ts",
    "tests:vitest:ci": "vitest --config ./lib/integrations/Vitest/vitest.config.ts --run --passWithNoTests",
    "tests:lint": "eslint ./ --fix",
    "tests:ts-types": "tsc --noemit",
    "tests:all": "COMMIT_SITEMAP=true npm run tests:ts-types && npm run tests:lint && npm run tests:vitest:ci && npm run build",
    "prettify": "tsx ./devtools/prettier/scripts/runPrettier/runPrettier.ts",
    "prepare": "tsx ./devtools/scripts/prepare/run.ts",
    "helpers:generate-logo-assets": "tsx ./devtools/helpers/assets/generateLogoAssets/run.ts",
    "helpers:create-icon": "tsx ./devtools/helpers/createIcon/run.ts",
    "helpers:update-useweb-deps": " npm i @jeremytenjo/super-code-generator@latest &&npm-check-updates -f /@useweb/*/ -u && npm i && npm run prettify && npm run tests:ts-types",
    "helpers:remove-empty-folders": "tsx ./devtools/helpers/files/removeEmptyFoldersInSrc/run.ts",
    "helpers:generate-blurred-public-images-copies": "tsx ./devtools/helpers/assets/generateBlurredPublicImagesCopies/run.ts"
  },
